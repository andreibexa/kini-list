{
  "name": "kini-list",
  "version": "1.0.0",
  "private": true,
  "description": "A list with movie trailers. Includes a filter for Provider and Country",
  "license": "",
  "author": "Andrei Bexa",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "lint": "typescript && eslint './src/**/*.{ts,tsx}' --cache --config --color .eslintrc.js",
    "lint-staged": "npm run prettier-format",
    "lint:fix": "eslint  './src/**/*.{ts,tsx}' --cache --fix --config .eslintrc.js",
    "prepare": "husky install",
    "prettier-format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' && npm run lint:fix",
    "serve": "serve -s build",
    "start": "react-scripts start",
    "typescript": "tsc --project tsconfig.json --noEmit"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@splidejs/react-splide": "^0.7.12",
    "@tanstack/react-query": "^4.7.1",
    "axios": "^1.5.0",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hot-toast": "^2.2.0",
    "react-player": "^2.10.0",
    "react-render-if-visible": "^2.1.1",
    "react-router-dom": "^6.3.0",
    "serve": "^14.2.1",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "@tanstack/eslint-plugin-query": "^4.13.0",
    "@tanstack/react-query-devtools": "^4.7.1",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/react-list": "^0.8.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-plugin-direct-import": "^1.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-schema-validator": "^4.6.0",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "themoviedb-typescript": "^1.0.2"
  }
}
